{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "EnvironmentPrefix": {
      "type": "String"
    },
    "EnvironmentType": {
      "type": "string",
      "defaultValue": "dev",
      "allowedValues": [
        "dev",
        "stg",
        "tst",
        "lab",
        "prd"
      ]
    },
    "FunctionType": {
      "type": "String",
      "defaultValue": "app",
      "allowedValues": [
        "app",
        "db",
        "web",
        "srv"
      ]
    },
    "NumberSuffix": {
      "type": "string",
      "defaultValue": "01",
      "allowedValues": [
        "01",
        "02",
        "03",
        "04",
        "05",
        "06",
        "07",
        "08",
        "09",
        "10",
        "11",
        "12"
      ]
    },
    "Location": {
      "type": "String",
      "defaultValue": "West Europe",
      "allowedValues": [
        "West Europe",
        "North Europe"
      ]
    }
  },
  "variables": {
    "satemplatelink": "https://raw.githubusercontent.com/jonaserikson/azure/master/Nested/redeploy-newstorageaccount.json"
  },
  "resources": [
    {
      "apiVersion": "2015-01-01",
      "name": "nestedTemplate",
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "incremental",
        "templateLink": {
          "uri": "[variables('satemplatelink')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "EnvironmentPrefix": {
            "value": "[parameters('EnvironmentPrefix')]"
          },
          "EnvironmentType": {
            "value": "[parameters('EnvironmentType')]"
          },
          "FunctionType": {
            "value": "[parameters('FunctionType')]"
          },
          "NumberSuffix": {
            "value": "[parameters('NumberSuffix')]"
          },
          "Location": {
            "value": "[parameters('Location')]"
          }
        }
      }
    }
  ]
}